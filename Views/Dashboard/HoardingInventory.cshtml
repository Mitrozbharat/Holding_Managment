                       function Openlivepreview(imageSrc, city, area, width, height, rate, vendorName, status) {
            // Set the image source for the modal
            $('#previewImage').attr('src', imageSrc);

            // Populate other fields in the modal
            $('#modalCity').text(city ? city : 'N/A');
            $('#modalArea').text(area ? area : 'N/A');
            $('#modalSize').text(`${width} FT X ${height} FT`);
            $('#modalRate').text(rate ? rate : 'N/A');
            $('#modalVendor').text(vendorName ? vendorName : 'N/A');

            // Set status based on its value
            if (status == 1) {
                status = "Available";
            } else if (status == 0) {
                status = "Booked";
            } else {
                status = "N/A"; // Default in case status is neither 0 nor 1
            }

            $('#modalStatus').text(status); // Set the status text

            // Show the modal
            $('#previewModal').modal('show');
        }







