        // Add city name to list and array
        function addItem() {
            const cityFilter = document.getElementById('cityFilter');
            const itemValue = cityFilter.value.trim();

            if (itemValue) {
                cityNames.push(itemValue);

                const listItem = document.createElement('li');
                listItem.classList.add('pl-2', 'custom-list-style', 'm-1');
                listItem.textContent = itemValue;

                const removeButton = document.createElement('span');
                removeButton.classList.add('bi-x', 'bg-color', 'remove-item', 'pl-2');
                removeButton.style.cursor = 'pointer';
                removeButton.style.padding = '5px';
                removeButton.onclick = function () {
                    cityNames.splice(cityNames.indexOf(itemValue), 1);
                    listItem.remove();
                };

                listItem.appendChild(removeButton);
                document.getElementById('listCity').appendChild(listItem);
                cityFilter.value = '';
            }
        }

        // Add area name to list and array
        function addArea() {
            const areaFilter = document.getElementById('areaFilter');
            const areaValue = areaFilter.value.trim();

            if (areaValue) {
                areaNames.push(areaValue);

                const listItem = document.createElement('li');
                listItem.classList.add('p-1', 'custom-list-style', 'm-1');
                listItem.textContent = areaValue;

                const removeButton = document.createElement('span');
                removeButton.classList.add('bi-x', 'bg-color', 'remove-item');
                removeButton.style.cursor = 'pointer';
                removeButton.style.padding = '5px';
                removeButton.style.marginLeft = '10px';
                removeButton.onclick = function () {
                    const index = areaNames.indexOf(areaValue);
                    if (index > -1) areaNames.splice(index, 1);
                    listItem.remove();
                };

                listItem.appendChild(removeButton);
                document.getElementById('listArea').appendChild(listItem);
                areaFilter.value = '';
            }
        }

